{
    "id": "mattermost-plugin-template",
    "name": "Mattermost plugin template",
    "description": "Mattermost plugin template",
    "icon_path": "assets/starter-template-icon.svg",
    "version": "0.0.1",
    "min_server_version": "6.5.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "header": "",
        "footer": "",
        "settings": [
            {
                "key": "mockClientBaseURL",
                "display_name": "Base URL of the mock client API",
                "type": "text",
                "help_text": "Enter base URL of the mock client API",
                "default": "https://jsonplaceholder.typicode.com"
            }
        ]
    }
}
